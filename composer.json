{
    "name": "evgenidev/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "wbraganca",
        "evgenidev",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "license": "proprietary",
    "support": {
        "issues": "https://github.com/evgenidev/yii2-dynamicform/issues",
        "wiki": "https://github.com/evgenidev/yii2-dynamicform/wiki/",
        "source": "https://github.com/evgenidev/yii2-dynamicform"
    },
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Miroshnichenko Evgeniy",
            "email": "evgeni_mir@mail.ru"
        }
    ],
    "require": {
        "ext-dom": "*",
        "yiisoft/yii2": "~2.0.52",
        "symfony/css-selector": "7.2.0",
        "symfony/dom-crawler": "7.2.4"
    },
    "autoload": {
        "psr-4": {
            "EvgeniDev\\Yii2\\DynamicForm\\": "src"
        }
    },
	"replace": {
		"wbraganca/yii2-dynamicform":"2.0.*"
	}
}
